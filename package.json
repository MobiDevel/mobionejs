{
  "name": "@mobione/mobionejs",
  "version": "1.0.2",
  "description": "JavaScript / TypeScript  models for integrating with MobiOne transportation and temperature systems",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./loads/*": {
      "types": "./dist/loads/*.d.ts",
      "import": "./dist/loads/*.mjs",
      "require": "./dist/loads/*.cjs"
    },
    "./orders/*": {
      "types": "./dist/orders/*.d.ts",
      "import": "./dist/orders/*.mjs",
      "require": "./dist/orders/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build:ts": "tsup --config tsup.config.ts",
    "build:watch": "tsup --config tsup.config.ts --watch",
    "build": "npm run clean && npm run build:ts",
    "changelog:generate": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "changelog:release": "npm run changelog:generate && git add CHANGELOG.md && git commit -m 'chore: update changelog' || echo 'No changelog changes'",
    "check:changelog": "node ./scripts/check-changelog.mjs",
    "check:exports": "node ./scripts/check-exports.mjs",
    "check:legacy-imports": "node ./scripts/check-legacy-imports.mjs",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "codemod:namespace": "node ./scripts/codemods/namespace-imports.mjs --write",
    "contr:add": "all-contributors add",
    "contr:generate": "all-contributors generate",
    "docs:toc": "npm run update:toc && git add **/*.md",
    "generate:symbol-map": "node ./scripts/generate-symbol-map.mjs",
    "lint:ci": "eslint src",
    "lint": "prettier -c -w src && eslint src --fix",
    "minify": "node ./scripts/esbuild.mjs",
    "prepack": "npm run build",
    "prepare": "npm run build && husky",
    "release:major": "node ./scripts/release-bump.mjs major",
    "release:minor": "node ./scripts/release-bump.mjs minor",
    "release:patch": "npm run version:next",
    "size": "size-limit",
    "start": "npm run build:watch",
    "test:cov": "npm run test --coverage",
    "test:debug": "jest -u --forceExit --coverage=false --debug --runInBand --onlyFailures --watchman=false",
    "test:dev": "jest --maxWorkers=25% --watchman=false",
    "test:watch": "jest --watchAll --maxWorkers=25% --watchman=false",
    "test": "jest --forceExit --runInBand --ci --watchman=false",
    "update:toc": "node ./scripts/update-toc.mjs && git add README.md",
    "verify:strict": "npm run verify && npm run check:legacy-imports && npm run check:changelog",
    "verify": "npm run clean && npm run lint && npm run test:dev && npm run build && npm run check:exports && size-limit",
    "version:next": "node ./scripts/version-next.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MobiDevel/mobionejs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MobiDevel/mobionejs/issues"
  },
  "homepage": "https://github.com/MobiDevel/mobionejs#readme",
  "sideEffects": false,
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "100 KB"
    }
  ],
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "all-contributors-cli": "^6.20.0",
    "babel-jest": "^30.1.2",
    "babel-loader": "^10.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "conventional-changelog-cli": "^5.0.0",
    "core-js": "^3.40.0",
    "dayjs": "^1.11.13",
    "esbuild": "^0.25.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "expo": "~54.0.20",
    "expo-status-bar": "~2.2.3",
    "glob": "^11.0.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "jest-junit-reporter": "^1.1.0",
    "json-schema-to-typescript": "^15.0.4",
    "mockdate": "^3.0.5",
    "prettier": "^3.5.3",
    "prettier-2": "npm:prettier@^2",
    "rimraf": "^6.0.1",
    "rollup": "^2.70.0",
    "size-limit": "^11.1.6",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.27.0",
    "uuid": "^11.1.0",
    "weak-napi": "^2.0.2",
    "zod": "^3.24.4",
    "zod-to-json-schema": "^3.24.5"
  }
}
